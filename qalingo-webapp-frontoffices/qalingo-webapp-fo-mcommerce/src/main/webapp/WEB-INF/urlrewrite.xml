<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
		
<urlrewrite>
	
	<!--
		CATEGORY RULE
	-->
	<rule match-type="regex">
		<name>Category Rule</name>
		<note></note>
		<from>^/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/product-axe-([a-z0-9-]+)(.html[^/]*)$</from>
		<to>/sc/product-axe.html?market-place-code=$1&amp;market-code=$2&amp;market-area-code=$3&amp;locale-code=$4&amp;retailer-code=$5&amp;category-code=$8</to>
	</rule>
	
	<rule match-type="regex">
		<name>Category Rule</name>
		<note></note>
		<from>^/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/product-line-([a-z0-9-]+)(.html[^/]*)$</from>
		<to>/sc/product-line.html?market-place-code=$1&amp;market-code=$2&amp;market-area-code=$3&amp;locale-code=$4&amp;retailer-code=$5&amp;category-code=$8</to>
	</rule>
	
	<!--
		PRODUCT RULE
	-->
	<rule match-type="regex">
		<name>Product Rule</name>
		<note></note>
		<from>^/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/product-details-([a-z0-9-]+)-([a-z0-9-]+)(.html[^/]*)$</from>
		<to>/sc/product-details.html?market-place-code=$1&amp;market-code=$2&amp;market-area-code=$3&amp;locale-code=$4&amp;retailer-code=$5&amp;category-code=$9&amp;product-code=$10</to>
	</rule>
	<rule match-type="regex">
		<name>Product Add To Cart Rule</name>
		<note></note>
		<from>^/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/add-to-cart-([a-z0-9-]+)-([a-z0-9-]+)-([a-z0-9-]+)(.html[^/]*)$</from>
		<to>/sc/add-to-cart.html?market-place-code=$1&amp;market-code=$2&amp;market-area-code=$3&amp;locale-code=$4&amp;retailer-code=$5&amp;category-code=$9&amp;product-code=$10&amp;product-sku-code=$11</to>
	</rule>
	<rule match-type="regex">
		<name>Product Remove From Cart Rule</name>
		<note></note>
		<from>^/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/remove-from-cart-([a-z0-9-]+)(.html[^/]*)$</from>
		<to>/sc/remove-from-cart.html?market-place-code=$1&amp;market-code=$2&amp;market-area-code=$3&amp;locale-code=$4&amp;retailer-code=$5&amp;item-position=$9</to>
	</rule>
	<rule match-type="regex">
		<name>Product Add To Wishlist Rule</name>
		<note></note>
		<from>^/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/add-to-wishlist-([a-z0-9-]+)-([a-z0-9-]+)-([a-z0-9-]+)(.html[^/]*)$</from>
		<to>/sc/add-to-wishlist.html?market-place-code=$1&amp;market-code=$2&amp;market-area-code=$3&amp;locale-code=$4&amp;&amp;retailer-code=$5category-code=$9&amp;product-code=$10&amp;product-sku-code=$11</to>
	</rule>
	<rule match-type="regex">
		<name>Product Remove From Wishlist Rule</name>
		<note></note>
		<from>^/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/remove-from-wishlist-([a-z0-9-]+)(.html[^/]*)$</from>
		<to>/sc/remove-from-wishlist.html?market-place-code=$1&amp;market-code=$2&amp;market-area-code=$3&amp;locale-code=$4&amp;retailer-code=$5&amp;item-position=$9</to>
	</rule>
	
	<!--
		BRAND RULE
	-->
	<rule match-type="regex">
		<name>Brand  Line Rule</name>
		<note></note>
		<from>^/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/brand-line-([a-z0-9-]+)(.html[^/]*)$</from>
		<to>/sc/brand-line.html?market-place-code=$1&amp;market-code=$2&amp;market-area-code=$3&amp;locale-code=$4&amp;retailer-code=$5&amp;brand-code=$8</to>
	</rule>
	
	<rule match-type="regex">
		<name>Brand Details Rule</name>
		<note></note>
		<from>^/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/brand-details-([a-z0-9-]+)(.html[^/]*)$</from>
		<to>/sc/brand-details.html?market-place-code=$1&amp;market-code=$2&amp;market-area-code=$3&amp;locale-code=$4&amp;retailer-code=$5&amp;brand-code=$8</to>
	</rule>
	
	<!--
		GLOBAL RULE / FALLBACK
	-->
	<rule match-type="regex">
		<name>Global Rule</name>
		<note></note>
		<from>^/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)/([a-z0-9-]+)(.html[^/]*)$</from>
		<to>/sc/$8.html?market-place-code=$1&amp;market-code=$2&amp;market-area-code=$3&amp;locale-code=$4</to>
	</rule>

</urlrewrite>

